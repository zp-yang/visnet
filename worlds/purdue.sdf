          <visualize>true</visualize>
          <topic>camera_2/image</topic>
        </sensor>
      </link>
    </model>

    <model name="camera_3">
      <pose>10 -10 10.0 0 0.0 2.35</pose>
      <static>1</static>
      <link name="link">
        <pose>0.1 0.05 0.05 0 0 0</pose>
        <inertial>
          <mass>0.1</mass>
          <inertia>
            <ixx>0.000166667</ixx>
